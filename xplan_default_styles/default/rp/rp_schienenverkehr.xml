  <Rule>
    <Name>VR_Haupteisenbahn</Name>
    <Description>
      <Title>Vorranggebiet Haupteisenbahnstrecke</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1002</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:gebietsTypCode</ogc:PropertyName>
          <ogc:Literal>1000</ogc:Literal>
        </ogc:PropertyIsEqualTo>
      </ogc:And>
    </ogc:Filter>
    <MinScaleDenominator>450000</MinScaleDenominator>
    <MaxScaleDenominator>2000000</MaxScaleDenominator>
    <LineSymbolizer>
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Stroke>
        <SvgParameter name="stroke">#8C0082</SvgParameter>
        <SvgParameter name="stroke-width">4</SvgParameter>
        <SvgParameter name="stroke-opacity">1</SvgParameter>
      </Stroke>
    </LineSymbolizer>
  </Rule>

