<FeatureTypeStyle xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.opengis.net/se"
  xmlns:ogc="http://www.opengis.net/ogc" xmlns:sed="http://www.deegree.org/se"
  xmlns:xplan="http://www.deegree.org/xplanung/1/0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd http://www.deegree.org/se http://schemas.deegree.org/se/1.1.0/Symbolizer-deegree.xsd">
  <Name>vollflaechig</Name>
  <FeatureTypeName>xplan:FP_GenerischesObjekt</FeatureTypeName>

  <!-- GenerischesObjekt Flaeche schluss == true -->
  <Rule>
    <Name>fp_genobj</Name>
    <Description>
      <Title>FP: Generisches Objekt</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:Function name="IsSurface">
            <ogc:PropertyName>xplan:position</ogc:PropertyName>
          </ogc:Function>
          <ogc:Literal>true</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:flaechenschluss</ogc:PropertyName>
          <ogc:Literal>true</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:Or>
          <ogc:PropertyIsNull>
            <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
          </ogc:PropertyIsNull>
          <ogc:Not>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
              <ogc:Literal>true</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Not>
        </ogc:Or>
      </ogc:And>
    </ogc:Filter>
    <MaxScaleDenominator>50000</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <SvgParameter name="fill">#6100c1</SvgParameter>
        <SvgParameter name="fill-opacity">1.0</SvgParameter>
      </Fill>
      <Stroke>
        <SvgParameter name="stroke">#000000</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
        <SvgParameter name="stroke-width">2.5</SvgParameter>
        <SvgParameter name="stroke-linecap">butt</SvgParameter>
        <SvgParameter name="stroke-linejoin">round</SvgParameter>
      </Stroke>
      <sed:PerpendicularOffset type="Round">-3.75</sed:PerpendicularOffset>
    </PolygonSymbolizer>
  </Rule>

  <!-- GenerischesObjekt Flaeche schluss == false -->
  <Rule>
    <Name>fp_genobj</Name>
    <Description>
      <Title>FP: Generisches Objekt</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:Function name="IsSurface">
            <ogc:PropertyName>xplan:position</ogc:PropertyName>
          </ogc:Function>
          <ogc:Literal>true</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:Or>
          <ogc:PropertyIsEqualTo>
            <ogc:PropertyName>xplan:flaechenschluss</ogc:PropertyName>
            <ogc:Literal>false</ogc:Literal>
          </ogc:PropertyIsEqualTo>
          <ogc:PropertyIsNull>
            <ogc:PropertyName>xplan:flaechenschluss</ogc:PropertyName>
          </ogc:PropertyIsNull>
        </ogc:Or>
        <ogc:Or>
          <ogc:PropertyIsNull>
            <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
          </ogc:PropertyIsNull>
          <ogc:Not>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
              <ogc:Literal>true</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Not>
        </ogc:Or>
      </ogc:And>
    </ogc:Filter>
    <MaxScaleDenominator>50000</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Stroke>
        <SvgParameter name="stroke">#6100c1</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
        <SvgParameter name="stroke-width">5</SvgParameter>
        <SvgParameter name="stroke-linecap">butt</SvgParameter>
        <SvgParameter name="stroke-linejoin">bevel</SvgParameter>
      </Stroke>
      <sed:PerpendicularOffset type="Edged">-2.5</sed:PerpendicularOffset>
    </PolygonSymbolizer>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Stroke>
        <SvgParameter name="stroke">#000000</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
        <SvgParameter name="stroke-width">1.875</SvgParameter>
        <SvgParameter name="stroke-linecap">butt</SvgParameter>
        <SvgParameter name="stroke-linejoin">round</SvgParameter>
      </Stroke>
    </PolygonSymbolizer>
  </Rule>

  <!-- FP_GenerischesObjekt Linie -->
  <Rule>
    <Name>fp_genobj</Name>
    <Description>
      <Title>FP: Generisches Objekt</Title>
    </Description>
    <ogc:Filter>
      <ogc:PropertyIsEqualTo>
        <ogc:Function name="IsCurve">
          <ogc:PropertyName>xplan:position</ogc:PropertyName>
        </ogc:Function>
        <ogc:Literal>true</ogc:Literal>
      </ogc:PropertyIsEqualTo>
    </ogc:Filter>
    <MaxScaleDenominator>50000</MaxScaleDenominator>
    <LineSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Stroke>
        <SvgParameter name="stroke">#6100C1</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
        <SvgParameter name="stroke-width">2.5</SvgParameter>
        <SvgParameter name="stroke-linecap">butt</SvgParameter>
        <SvgParameter name="stroke-linejoin">round</SvgParameter>
      </Stroke>
    </LineSymbolizer>
  </Rule>

  <!-- GenerischesObjekt Point-->
  <Rule>
    <Name>fp_genobj</Name>
    <Description>
      <Title>FP: Generisches Objekt</Title>
    </Description>
    <ogc:Filter>
      <ogc:PropertyIsEqualTo>
        <ogc:Function name="IsPoint">
          <ogc:PropertyName>xplan:position</ogc:PropertyName>
        </ogc:Function>
        <ogc:Literal>true</ogc:Literal>
      </ogc:PropertyIsEqualTo>
    </ogc:Filter>
    <MaxScaleDenominator>50000</MaxScaleDenominator>
    <PointSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Graphic>
        <Mark>
          <OnlineResource xlink:href="../../symbols/FreeSans.ttf" />
          <Format>ttf</Format>
          <MarkIndex>42</MarkIndex>
          <Fill>
            <SvgParameter name="fill">#6100c1</SvgParameter>
          </Fill>
          <Stroke>
            <SvgParameter name="stroke-opacity">0</SvgParameter>
          </Stroke>
        </Mark>
        <Size>50</Size>
      </Graphic>
    </PointSymbolizer>
    <PointSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Graphic>
        <Mark>
          <WellKnownName>circle</WellKnownName>
          <Fill>
            <SvgParameter name="fill">#6100c1</SvgParameter>
            <SvgParameter name="fill-opacity">0.0</SvgParameter>
          </Fill>
          <Stroke>
            <SvgParameter name="stroke">#6100c1</SvgParameter>
            <SvgParameter name="stroke-opacity">1.0</SvgParameter>
            <SvgParameter name="stroke-width">2</SvgParameter>
          </Stroke>
        </Mark>
        <Size>70</Size>
      </Graphic>
    </PointSymbolizer>
  </Rule>
  <!-- vonGenehmigungAusgenommen -->
  <Rule>
    <Name>VonGenehmigungAusgenommen</Name>
    <Description>
      <Title>FP: VonGenehmigungAusgenommen</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:Function name="IsSurface">
            <ogc:PropertyName>xplan:position</ogc:PropertyName>
          </ogc:Function>
          <ogc:Literal>true</ogc:Literal>
        </ogc:PropertyIsEqualTo>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:vonGenehmigungAusgenommen</ogc:PropertyName>
          <ogc:Literal>true</ogc:Literal>
        </ogc:PropertyIsEqualTo>
      </ogc:And>
    </ogc:Filter>
    <MaxScaleDenominator>100000</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <SvgParameter name="fill">#FFFFFF</SvgParameter>
        <SvgParameter name="fill-opacity">1.0</SvgParameter>
      </Fill>
      <Stroke>
        <SvgParameter name="stroke">#000000</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
        <SvgParameter name="stroke-width">1.0</SvgParameter>
      </Stroke>
    </PolygonSymbolizer>
  </Rule>
</FeatureTypeStyle>
