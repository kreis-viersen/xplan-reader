    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Stroke>
        <GraphicStroke>
		  <Graphic>
			<Mark>
			  <OnlineResource xlink:href="../../symbols/FreeSans.ttf" />
			  <Format>ttf</Format>
			  <MarkIndex>34</MarkIndex>
			  <Fill>
				<SvgParameter name="fill">#000000</SvgParameter>
			  </Fill>
			  <Stroke>
				<SvgParameter name="stroke-opacity">0</SvgParameter>
			  </Stroke>
			</Mark>
			<Size>2</Size>
		  </Graphic>
          <Gap>8</Gap>
        </GraphicStroke>
      </Stroke>
      <sed:PerpendicularOffset type="Standard">-1</sed:PerpendicularOffset>
    </PolygonSymbolizer>
