        <SvgParameter name="stroke-width">2</SvgParameter>
        <SvgParameter name="stroke-linecap">butt</SvgParameter>
        <SvgParameter name="stroke-dasharray">0 1 2 2</SvgParameter>
        <SvgParameter name="stroke-dashoffset">1</SvgParameter>
      </Stroke>
      <sed:PerpendicularOffset type="Standard">-1</sed:PerpendicularOffset>
    </PolygonSymbolizer>
    <PolygonSymbolizer uom="meter">
