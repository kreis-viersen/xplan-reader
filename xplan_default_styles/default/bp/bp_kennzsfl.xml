  <!-- SchadstoffBelastBoden -->
  <Rule>
    <Name>SchadstoffBelastBoden</Name>
    <Description>
      <Title>BP: Schadstoffbelasteter Boden</Title>
    </Description>
    <ogc:Filter>
      <ogc:And>
        <ogc:PropertyIsEqualTo>
          <ogc:Function name="IsSurface">
            <ogc:PropertyName>xplan:position</ogc:PropertyName>
          </ogc:Function>
