<FeatureTypeStyle xmlns="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:xplan="http://www.deegree.org/xplanung/1/0"
  xmlns:sed="http://www.deegree.org/se"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/se/1.1.0/FeatureStyle.xsd http://www.deegree.org/se http://schemas.deegree.org/se/1.1.0/Symbolizer-deegree.xsd">
  <Name>vollflaechig</Name>
  <FeatureTypeName>xplan:BP_SpezielleBauweise</FeatureTypeName>
  <!-- TODO: platzhalter -->
  <Rule>
    <Name>bp_speziellebauweise</Name>
    <Description>
      <Title>BP: Spezielle Bauweise</Title>
    </Description>
    <MaxScaleDenominator>2500</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Stroke>
        <SvgParameter name="stroke">#000000</SvgParameter>
        <SvgParameter name="stroke-width">0.2</SvgParameter>
        <SvgParameter name="stroke-opacity">1.0</SvgParameter>
      </Stroke>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Name>speziellebauweise</Name>
    <Description>
      <Title>Durchgang, Durchfahrt, Luftgeschoss, Überbauung, Brücke, Tunnel</Title>
    </Description>
    <ogc:Filter>
	  <ogc:Or>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1000</ogc:Literal>
        </ogc:PropertyIsEqualTo>
		<ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1100</ogc:Literal>
        </ogc:PropertyIsEqualTo>
		<ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1200</ogc:Literal>
        </ogc:PropertyIsEqualTo>
		<ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1500</ogc:Literal>
        </ogc:PropertyIsEqualTo>
		<ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1600</ogc:Literal>
        </ogc:PropertyIsEqualTo>
		<ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1700</ogc:Literal>
        </ogc:PropertyIsEqualTo>
	  </ogc:Or>
    </ogc:Filter>
    <MaxScaleDenominator>1000</MaxScaleDenominator>
	<PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <GraphicFill>
          <Graphic>
            <ExternalGraphic>
              <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple"
                xlink:href="../../symbols/speziellebauweise_bruecke.png" />
              <Format>image/png</Format>
            </ExternalGraphic>
            <!--Size>26.3</Size-->
          </Graphic>
        </GraphicFill>
      </Fill>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Name>speziellebauweise</Name>
    <Description>
      <Title>Auskragung</Title>
    </Description>
    <ogc:Filter>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1300</ogc:Literal>
        </ogc:PropertyIsEqualTo>
    </ogc:Filter>
    <MaxScaleDenominator>1000</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <GraphicFill>
          <Graphic>
            <ExternalGraphic>
              <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple"
                xlink:href="../../symbols/speziellebauweise_auskragung.png" />
              <Format>image/png</Format>
            </ExternalGraphic>
            <!--Size>26.3</Size-->
          </Graphic>
        </GraphicFill>
      </Fill>
    </PolygonSymbolizer>
  </Rule>
  <Rule>
    <Name>speziellebauweise</Name>
    <Description>
      <Title>Arkade</Title>
    </Description>
    <ogc:Filter>
        <ogc:PropertyIsEqualTo>
          <ogc:PropertyName>xplan:typCode</ogc:PropertyName>
          <ogc:Literal>1400</ogc:Literal>
        </ogc:PropertyIsEqualTo>
    </ogc:Filter>
    <MaxScaleDenominator>1000</MaxScaleDenominator>
    <PolygonSymbolizer uom="meter">
      <Geometry>
        <ogc:PropertyName>xplan:position</ogc:PropertyName>
      </Geometry>
      <Fill>
        <GraphicFill>
          <Graphic>
            <ExternalGraphic>
              <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple"
                xlink:href="../../symbols/speziellebauweise_arkade.png" />
              <Format>image/png</Format>
            </ExternalGraphic>
            <!--Size>26.3</Size-->
          </Graphic>
        </GraphicFill>
      </Fill>
    </PolygonSymbolizer>
  </Rule>
